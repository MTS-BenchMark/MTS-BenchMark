    "optim": "adam",
    "lr": 0.001,
    "L1Loss": true,
    "patience": 20,
    "lr_decay": false,
    "lr_scheduler": "multisteplr",
    "lr_decay_ratio": 0.1,
    "lr_decay_steps": [5, 20, 40, 70],
    "step_size": 10
  }
  