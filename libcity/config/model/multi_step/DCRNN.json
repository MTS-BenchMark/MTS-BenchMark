{
    "device": "cuda:0",
    "gpu": true,
    "gpu_id": 0,
    "save": true,
    "cuda": true,
  
    "normalize": 1,
    "window": 12,
    "horizon": 12,
    "batch_size": 64,
    "mask": true,

    "input_dim": 2,
    "output_dim": 1,
  
